{
    "name": "Application Service",
    "version": "1.0.13",
    "engines": {
        "npm": ">=9.5.0",
        "node": ">=22.8.0"
    },
    "scripts": {
        "start": "node ./bin/www",
        "start:dev": "nodemon -L -e .js,.json,.njk,.yml  --exec npm run build:run:dev",
        "build:run:dev": "node --inspect=0.0.0.0:9229 ./bin/www",
        "test": "jest",
        "coverage": "jest --coverage",
        "dump-eslint-conf": "eslint --print-config ./app.js > ./stuff/eslint-conf.json",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.400.0",
        "@aws-sdk/client-sqs": "^3.398.0",
        "@aws-sdk/util-stream-node": "^3.350.0",
        "cheerio": "^1.0.0",
        "cookie-parser": "~1.4.3",
        "debug": "^4.3.4",
        "dotenv": "^16.1.3",
        "express": "^4.21.0",
        "helmet": "^3.21.2",
        "pdfkit": "^0.13.0",
        "pino": "^8.14.1",
        "verror": "^1.10.0"
    },
    "devDependencies": {
        "aws-sdk-client-mock": "^2.1.1",
        "eslint": "^8.0.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-fp": "^2.3.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jest": "^23.3.0",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.0.3",
        "jest": "^28.1.3",
        "json-schema-ref-parser": "^6.1.0",
        "lint-staged": "^11.0.0",
        "nodemon": "^3.1.4",
        "prettier": "^1.19.1",
        "yamljs": "^0.3.0"
    },
    "license": "MIT"
